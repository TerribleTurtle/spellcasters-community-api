{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "name", "game_version"],
  "properties": {
    "id": { "type": "string" },
    "name": { "type": "string" },
    "description": { "type": "string" },
    "spellcaster_id": { "type": "string" },
    "cards": {
      "type": "array",
      "items": { "type": "string" }
    },
    "game_version": { "type": "string" }
  },
  "additionalProperties": false
}
